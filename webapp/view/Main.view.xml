<mvc:View controllerName="dk.sorenviggo.ui5.jspdf.jsPDF_Create.controller.Main" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<VBox>
							<HBox>
								<Button text="Open PDF" press="onOpenPDF"/>
								<Button text="Save PDF" press="onSavePDF"/>
							</HBox>
						</VBox>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>